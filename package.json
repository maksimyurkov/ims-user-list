{
  "name": "@maksimyurkov/ims-user-list",
  "version": "0.1.1",
  "description": "User List web component for WebTutor and other LMS",
  "main": "ims-user-list.js",
  "module": "ims-user-list.js",
  "scripts": {
    "build": "webpack --env production",
    "server": "cd api && nodemon nodejs-dev-server.js",
    "client":
      "polyserve --module-resolution=node --open --open-path ./demo/development.html",
    "dev":
      "concurrently \"webpack --env development\" \"npm run server\" \"npm run client\"",
    "postinstall": "node utils/generate-helpers.js && npm run build",
    "lint": "eslint ./*.js utils/*.js api/*.js",
    "demo": "polyserve --open --open-path ./demo/",
    "start": "polyserve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maksimyurkov/ims-user-list.git"
  },
  "keywords": ["module", "plugin", "lms", "web component", "webtutor"],
  "author": "maksimyurkov",
  "license": "Elements Personal Free License (EPFL)",
  "bugs": {
    "url": "https://github.com/maksimyurkov/ims-user-list/issues"
  },
